            android:label="Trainer Form"/>
        <activity android:name=".TrainerDetailsActivity"
            android:label="Trainer Details"/>
        <activity android:name=".MemberUpdateActivity"
            android:label="Update Member"/>
        <activity android:name=".MemberFormActivity"
            android:label="Member Form"/>
        <activity android:name=".MemberDetailsActivity"
            android:label="Member Details"/>
        <activity android:name=".AddGymActivity"
            android:label="Gym Info"/>
        <activity android:name=".StartActivity"
            android:label="Gym Management"/>
        <activity android:name=".MainActivity"
            android:label="Gym Info">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>