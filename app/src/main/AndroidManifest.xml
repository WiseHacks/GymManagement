        android:supportsRtl="true"
        android:theme="@style/Theme.TestingProject">
        <activity android:name=".WelcomeActivity"
            android:label="Welcome"/>
        <activity android:name=".EquipmentUpdateActivity"
            android:label="Update Equipment"/>
        <activity android:name=".EquipmentFormActivity"
            android:label="Equipment Form"/>
        <activity android:name=".EquipmentDetailsActivity"
            android:label="Equipment Details"/>
        <activity android:name=".PlanUpdateActivity"
            android:label="Update Plan"/>
        <activity android:name=".PlanFormActivity"
            android:label="Plan Form"/>
        <activity android:name=".PlanDetailsActivity"
